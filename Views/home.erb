<div class="search">
	<img src="/images/Bloghubcomplete.png" width="200" height="120" >
	<textarea class="textarea" >Search</textarea>

</div>

<div class="feed">
<h1>feed</h1>
<p><a href="/profile"><%= @current_user.first + " " +  @current_user.last %></a></p>
<% @current_user.posts.each do |item| %>
<p>Title: <%=item.title %></p>
<p>Body: <%=item.body %></p>
<p>Time: <%=item.created_at.strftime("%B %d, %Y?") %></p>

<% end %>
	
<% @posts.each do |post| %>


<p><a href="/users/<%= post.user.id %>">
	<%= post.user.first + " " + post.user.last %></a></p>
		<strong><%= post.title %></strong>
		<br>
		<%= post.genre %>
		<br>
		<%= post.body %>
		<br>
		<%=post.created_at.strftime("%B %d, %Y") %></p>



	

<% end %>
</div>
<div class="users">
	<h3>Users<h3>

<% @users.each do |user| %>
<p><a href="/users/<%= user.id %>">
	<%= user.first + " " + user.last %></a></p>
	

<% end %>

</div>